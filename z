AUERR = local_au(curdat, cmd);
QUIT

static int local_au(const char *curdat, const char* cmd)
{
	/* open intrng file in 'w' mode and completely overwrite existing cuts with 
	the ones supplied in the command line.  The command assumes the correct number of 
	arguments are passed in highppm lowppm order, whitespace separated
	*/
  FILE *intfile;
  intfile = fopen(PROCPATH("intrng"), "w");
  int high, low;
  char hi[1000];
  char lo[1000];
  
  if(intfile==NULL)
  {
  	Proc_err(DEF_ERR_OPT, "Can't write integrals:\n%s", PROCPATH("intrng"));
  	return(-1);
  }
  if(i_argv[2] != NULL)
  {
  	fprintf(intfile, "A 1.0 #regions in PPM\n");
  	fprintf(intfile, "# low field    high field  bias        slope\n");
  	for(high = 2; high < i_argc; high = high + 2)
  	{
  		low = high + 1;
  		fprintf(intfile, "%s %s 0 0\n", i_argv[high], i_argv[low]);
  	}
  }
  fclose(intfile);
	VIEWDATA_SAMEWIN;
	XCMD("sendgui browse_update_tree");
	return 0;
}